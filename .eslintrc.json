{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": 0,
    "object-curly-newline": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "newline-per-chained-call": 0,
    "no-unneeded-ternary": 0,
    "operator-linebreak": 0,
    "implicit-arrow-linebreak": 0
  }
}
